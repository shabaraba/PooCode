// 条件付き関数の修正テスト

// 条件付き関数 - 🍕 == 1 の場合
def test_conditional() if 🍕 == 1 : int -> string {
    "条件に一致しました: 🍕=" + to_string(🍕) >> 💩
}

// デフォルト関数
def test_conditional(): int -> string {
    "デフォルト関数: 🍕=" + to_string(🍕) >> 💩
}

// 複雑な条件付き関数 - 🍕 > 5 の場合
def complex_test() if 🍕 > 5 : int -> string {
    "🍕は5より大きいです: " + to_string(🍕) >> 💩
}

// 複雑な条件付き関数 - 🍕 <= 5 の場合
def complex_test() if 🍕 <= 5 : int -> string {
    "🍕は5以下です: " + to_string(🍕) >> 💩
}

// テスト実行
"==================== 条件式テスト =====================" |> print;

"--- 基本条件テスト: 🍕 == 1 ---" |> print;
0 |> test_conditional |> print;
1 |> test_conditional |> print;

"--- 複雑条件テスト: 🍕 > 5 vs 🍕 <= 5 ---" |> print;
3 |> complex_test |> print;
5 |> complex_test |> print;
6 |> complex_test |> print;
10 |> complex_test |> print;
