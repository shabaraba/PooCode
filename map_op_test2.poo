// map演算子のテスト
def double(): int -> int {
    🍕 * 2 >> 💩
}

// 単純な数値に関数を適用してみる
5 |> double |> print;

// map演算子を使ってみる
print("--- 次に +> 演算子を使用してみます ---");
[1, 2, 3] +> double |> print;

// ?> 演算子も使ってみる
def is_even(): int -> bool {
    🍕 % 2 == 0 >> 💩
}

print("--- ?> 演算子のテスト ---");
[1, 2, 3, 4, 5] ?> is_even |> print;
