// 配列機能のサンプル

// 基本的な配列作成
let arr1 = [1, 2, 3, 4, 5];
println("基本的な配列: " + arr1);

// map関数サンプル
let double = fn() { 🍕 * 2 };
println("Map関数（倍数）: " + (arr1 |> map double));

// filter関数サンプル
let isEven = fn() { 🍕 % 2 == 0 };
println("Filter関数（偶数）: " + (arr1 |> filter isEven));

// 複数パイプライン
println("偶数を抽出して倍数にする: " + (arr1 |> filter isEven |> map double));
