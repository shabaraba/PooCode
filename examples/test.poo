// 単純な条件付き関数テスト
def test_simple() if 🍕 == 1 : int -> int {
    10 >> 💩
}

def test_simple(): int -> int {
    20 >> 💩
}

// 明示的な等価比較テスト
def equals_1(x): int -> boolean {
    x == 1 >> 💩
}

// テスト実行
"入力 0 での条件式テスト:" |> print;
0 |> test_simple |> print;

"入力 1 での条件式テスト:" |> print;
1 |> test_simple |> print;

"明示的な等価比較:" |> print;
"0 == 1:" |> print;
0 |> equals_1 |> print;
"1 == 1:" |> print;
1 |> equals_1 |> print;
