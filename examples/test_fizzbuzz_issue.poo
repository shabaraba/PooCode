// テストケース1: 通常の定義と呼び出し
def showTest1() : int -> str {
    🍕 |> print;
    "" >> result;
    
    "Result 1: " + (🍕 |> to_string) >> 💩;
}

5 |> showTest1 |> print;
[1..3] +> showTest1 +> print;

