// 簡略化されたテスト版: 条件分岐による関数オーバーロード
def test(num) if 🍕 == 0 : int -> int {
    // 🍕が0の場合はこの関数が呼ばれる
    100 >> 💩 
}
def test(num) if 🍕 == 1 : int -> int {
    // 🍕が1の場合はこの関数が呼ばれる
    200 >> 💩
}
def test(num): int -> int {
    // それ以外の場合はこの関数が呼ばれる
    300 >> 💩 
}

// テスト: 値を表示
0 |> test(999) |> print;  // 100が返るはず 
1 |> test(999) |> print;  // 200が返るはず
2 |> test(999) |> print;  // 300が返るはず
