// 条件判定のテスト
// 整数の3の倍数判定
3 % 3 == 0 >> is_div3
is_div3 |> print

// 整数の3の倍数でない判定
4 % 3 == 0 >> is_not_div3
is_not_div3 |> print

// 真偽値の否定
not true >> not_true
not_true |> print

// 真偽値の論理積
true && true >> and_result
and_result |> print

// 真偽値の論理和
true || false >> or_result
or_result |> print

// 真偽値同士の比較
true eq true >> eq_result
eq_result |> print

// 整数の比較
3 eq 3 >> num_eq
num_eq |> print

// 整数の比較（不一致）
3 eq 4 >> num_neq
num_neq |> print
