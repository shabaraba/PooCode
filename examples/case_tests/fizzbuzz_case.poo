// FizzBuzzのcase文バージョン - 明示的条件

def fizzbuzz() {
  case 🍕 % 15 == 0: {
    "FizzBuzz" >> 💩
  }
  case 🍕 % 3 == 0: {
    "Fizz" >> 💩
  }
  case 🍕 % 5 == 0: {
    "Buzz" >> 💩
  }
  case 🍕 % 3 != 0 && 🍕 % 5 != 0: {
    🍕 |> to_string >> 💩
  }
}

// 1から20までのFizzBuzzを出力
1 |> fizzbuzz |> print;
2 |> fizzbuzz |> print;
3 |> fizzbuzz |> print;
4 |> fizzbuzz |> print;
5 |> fizzbuzz |> print;
6 |> fizzbuzz |> print;
7 |> fizzbuzz |> print;
8 |> fizzbuzz |> print;
9 |> fizzbuzz |> print;
10 |> fizzbuzz |> print;
