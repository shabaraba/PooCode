// Define a test function to double values
def double(): int -> int {
    🍕 * 2 >> 💩
}

// Define a test function to add a number
def add_num(num): int -> int {
    🍕 + num >> 💩
}

// Simply print each number individually
print("Program started");

// Use built-in print function with map
[1..10] |> map print;

// Use our custom double function with map
print("\nDoubled values:");
[1..10] |> map double |> map print;

// Use a function with argument
print("\nAdd 100 to each value:");
[1..10] |> map add_num(100) |> map print;

print("Program completed");
