def is_multiple_of(num): int -> bool {
    🍕 % num == 0 >> 💩
}

def showFizz() if 🍕 % 3 == 0 : int -> str{
    "Fizz" >> 💩
}
def showFizz() : int -> str{
    "" >> 💩
}
 
def showBuzz() if 🍕 % 5 == 0 : int -> str{
    "Buzz" >> 💩
}
def showBuzz() : int -> str{
    "" >> 💩
}
 
def fizzbuzz(): int -> str {
    "" >> result
    
    🍕 |> showFizz |> add result
    🍕 |> showBuzz |> add result >> 💩
}

