def showFizz() if 🍕 % 3 == 0 : int -> str {
    "Fizz" >> 💩;
}
def showFizz() : int -> str{
    "" >> 💩;
}
 
def showBuzz() if 🍕 % 5 == 0 : int -> str {
    "Buzz" >> 💩;
}
def showBuzz() : int -> str {
    "" >> 💩;
}

def showNumber() if (🍕 % 3 == 0) || (🍕 % 5 == 0) : int -> str {
    "" >> 💩;
}
def showNumber() : int -> str {
    🍕 |> to_string >> 💩;
}
 
def showTest() : int -> str {
    "" >> result;
    
    🍕 |> showFizz |> add result >> result;
    🍕 |> showBuzz |> add result >> result;
    🍕 |> showNumber |> add result >> result;
    
    result >> 💩;
}

[1..20] +> showTest +> print;