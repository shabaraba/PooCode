// シンプルなFizzBuzzプログラム（notを使わない実装）
def is_multiple_of(num): int -> bool {
    🍕 % num == 0 >> 💩
}

def fizzbuzz(num): int -> str {
    "" >> result
    
    🍕 is_multiple_of 3 >> is_div3
    num is_multiple_of 5 >> is_div5
    
    is_div3 && is_div5 >> is_div_both
    
    // 結果に応じて文字列を追加
    is_div_both eq true |> add "FizzBuzz" >> result
    is_div_both eq false && is_div3 eq true |> add "Fizz" >> result
    is_div_both eq false && is_div5 eq true |> add "Buzz" >> result
    
    // どれにも当てはまらない場合は数値を文字列として返す
    result eq "" |> add num |> to_string >> result
    
    result >> 💩
}

