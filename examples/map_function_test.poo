// map演算子(+>)での関数ルックアップのテスト

// 数値を文字列に変換する関数
def toString(): int -> str {
    🍕 |> to_string >> 💩
}

// 奇数かどうかをチェックする関数
def isOdd(): int -> bool {
    🍕 % 2 != 0 >> 💩
}

// 対象の引数を2倍にする関数 - 条件付き/デフォルト両方のバージョン
def double() if 🍕 > 5 : int -> int {
    "大きい数字を2倍: " |> print
    🍕 * 2 >> 💩
}

def double(): int -> int {
    "通常の数字を2倍: " |> print
    🍕 * 2 >> 💩
}

// パイプライン演算子のテスト - 基準として
"パイプライン演算子テスト:" |> print
3 |> double |> print
10 |> double |> print

// map演算子テスト - 配列に対して
"map演算子テスト:" |> print
[1..5] +> double +> print
[3, 8, 10, 15] +> double +> print

// 関数ルックアップとデバッグ情報
"map演算子でのtoString関数テスト:" |> print
[1..3] +> toString +> print

// 条件付き関数の動作確認
"map演算子での条件付き関数テスト:" |> print
[3, 5, 8, 12] +> double +> print

// filter演算子のテスト
"filter演算子テスト:" |> print
[1..10] ?> isOdd +> print